// note: "$()" in "https:/$()/" prevents the double slash being recognized as comment

#include "Base.xcconfig"

PHANTOM_CONFIG_NAME = debug

PHANTOM_APP_NAME = $(inherited)Dbg
PRODUCT_BUNDLE_IDENTIFIER = $(inherited).debug // system setting (not user-defined)

PHANTOM_DEVELOPER_NAME = $(inherited)
PHANTOM_DEVELOPER_CONTACT_EMAIL = $(inherited)
PHANTOM_DEVELOPER_REDDIT_ACCOUNT = $(inherited)
PHANTOM_DEVELOPER_TWITTER_URL = $(inherited)

PHANTOM_WEBSITE_URL = $(inherited)
PHANTOM_PRIVACY_POLICY_URL = $(inherited)
PHANTOM_APP_STORE_URL = $(inherited)

PHANTOM_REDDIT_CLIENT_ID = aaa
PHANTOM_REDDIT_REDIRECT_URI = https:/$()/aaa.com

PHANTOM_IMGUR_CLIENT_ID = aaa
PHANTOM_IMGUR_CLIENT_SECRET = aaa
PHANTOM_IMGUR_REDIRECT_URI = https:/$()/aaa.com

PHANTOM_DEBUG_SIMULATE_MIDDLEWARE = true
PHANTOM_DEBUG_SIMULATE_REDDIT = true
PHANTOM_DEBUG_DISABLE_RETRY = true
PHANTOM_DEBUG_DISABLE_DIRECT_IMGUR_UPLOAD = true
PHANTOM_DEBUG_SIMULATE_ERROR = false
PHANTOM_DEBUG_INSTANT_NOTIFICATIONS = false
